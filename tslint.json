{
  "extends": [
    "tslint-config-airbnb",
    "@polymer/tools-common/tslint.json"
  ],
  "rules": {
    "max-line-length": [true, 120],
    "object-shorthand-properties-first": false,
    "radix": false,
    "ter-arrow-parens": false,
    "ter-indent": [
      true, 4
    ],
    "trailing-comma": false
  }
}
